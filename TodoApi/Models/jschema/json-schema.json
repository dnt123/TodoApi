{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "http://example.com/root.json",
	"type": "object",
	"title": "The Root Schema",
	"required": [
		"id",
		"name",
		"isComplete"
	],
	"properties": {
		"id": {
			"$id": "#/properties/id",
			"type": "integer",
			"title": "The Id Schema",
			"default": 0,
			"examples": [
				1
			]
		},
		"name": {
			"$id": "#/properties/name",
			"type": "string",
			"title": "The Name Schema",
			"default": "",
			"examples": [
				"Item1"
			],
			"pattern": "^(.*)$"
		},
		"isComplete": {
			"$id": "#/properties/isComplete",
			"type": "boolean",
			"title": "The Iscomplete Schema",
			"default": false,
			"examples": [
				false
			]
		}
	}
}